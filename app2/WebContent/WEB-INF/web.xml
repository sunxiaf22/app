<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DetectIPServ</servlet-name>
    <servlet-class>org.church.our.loving.http.DetectIPServ</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ForwardReqeust</servlet-name>
    <servlet-class>org.church.our.loving.http.ForwardRequest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DetectIPServ</servlet-name>
    <url-pattern>/serv/updateips</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ForwardReqeust</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>SendMail</display-name>
    <servlet-name>SendMail</servlet-name>
    <servlet-class>org.church.our.loving.http.SendMail</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SendMail</servlet-name>
    <url-pattern>/SendMail</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>This servlet is used to handle file uploading cases.</description>
    <display-name>Upload</display-name>
    <servlet-name>Upload</servlet-name>
    <servlet-class>org.church.our.loving.http.Upload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DowloadFile</display-name>
    <servlet-name>DowloadFile</servlet-name>
    <servlet-class>org.church.our.loving.http.DowloadFile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DowloadFile</servlet-name>
    <url-pattern>/download</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ShowFileList</display-name>
    <servlet-name>ShowFileList</servlet-name>
    <servlet-class>org.church.our.loving.http.ShowFileList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowFileList</servlet-name>
    <url-pattern>/showfile</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ConfigureSetup</display-name>
    <servlet-name>ConfigureSetup</servlet-name>
    <servlet-class>org.church.our.loving.http.ConfigureSetup</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConfigureSetup</servlet-name>
    <url-pattern>/configureSetup</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Forward</display-name>
    <servlet-name>Forward</servlet-name>
    <servlet-class>org.church.our.loving.http.Forward</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Forward</servlet-name>
    <url-pattern>/forward</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Photo</display-name>
    <servlet-name>Photo</servlet-name>
    <servlet-class>org.church.our.loving.http.Photo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Photo</servlet-name>
    <url-pattern>/photo.do</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <page-encoding>UTF-8</page-encoding>
    </jsp-property-group>
  </jsp-config>
  <filter>
    <display-name>SecurityFilter</display-name>
    <filter-name>SecurityFilter</filter-name>
    <filter-class>org.church.our.loving.http.SecurityFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SecurityFilter</filter-name>
    <url-pattern>*.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>SecurityFilter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>Login</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>org.church.our.loving.http.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Status</display-name>
    <servlet-name>Status</servlet-name>
    <servlet-class>org.church.our.loving.http.Status</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Status</servlet-name>
    <url-pattern>/status</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RegisterDO</display-name>
    <servlet-name>RegisterDO</servlet-name>
    <servlet-class>org.church.our.loving.http.RegisterDO</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterDO</servlet-name>
    <url-pattern>/register.do</url-pattern>
  </servlet-mapping>
</web-app>